<!--Property: Modifying agency in Cataloguing Source-->
<rdf:Description rdf:about="http://marc21rdf.info/elements/0XX/M040__d">
  <rdfs:isDefinedBy rdf:resource="http://marc21rdf.info/elements/0XX" />
