<?xml version="1.0" encoding = "UTF-8"?>
<rdf:RDF
    xmlns="http://www.w3.org/2004/02/skos/core#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:reg="http://metadataregistry.org/uri/schema/registry/">

<!-- Scheme: MARC21-007: Color characteristics of a motion picture -->
    <skos:ConceptScheme rdf:about="http://marc21rdf.info/terms/motionpicturecol#">
        <dc:title>MARC21-007: Color characteristics of a motion picture</dc:title>
    </skos:ConceptScheme>

<!-- Concept: black-and-white  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturecol#b" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturecol#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="3539"/>
				<skos:prefLabel xml:lang="en">black-and-white</skos:prefLabel>
				<skos:definition xml:lang="en">Printed or executed in black-and-white.</skos:definition>
				<skos:notation xml:lang="en">b</skos:notation>
		</skos:Concept>
<!-- Concept: hand colored  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturecol#h" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturecol#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="3541"/>
				<skos:prefLabel xml:lang="en">hand colored</skos:prefLabel>
				<skos:definition xml:lang="en">Motion picture, produced by a printing or photographic process, is hand colored.</skos:definition>
				<skos:notation xml:lang="en">h</skos:notation>
				<skos:scopeNote xml:lang="en">Rarely used with commercial films since these are not typically issued with hand coloring.</skos:scopeNote>
		</skos:Concept>
<!-- Concept: mixed  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturecol#m" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturecol#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="3542"/>
				<skos:prefLabel xml:lang="en">mixed</skos:prefLabel>
				<skos:definition xml:lang="en">Work or collection is a combination of black-and-white, multicolored, hand colored, and/or other images.</skos:definition>
				<skos:notation xml:lang="en">m</skos:notation>
		</skos:Concept>
<!-- Concept: multicolored  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturecol#c" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturecol#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="3540"/>
				<skos:prefLabel xml:lang="en">multicolored</skos:prefLabel>
				<skos:definition xml:lang="en">Printed or executed in more than one color.</skos:definition>
				<skos:notation xml:lang="en">c</skos:notation>
				<skos:scopeNote xml:lang="en">Used for color photographic processes.</skos:scopeNote>
		</skos:Concept>
<!-- Concept: not applicable  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturecol#n" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturecol#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="3543"/>
				<skos:prefLabel xml:lang="en">not applicable</skos:prefLabel>
				<skos:definition xml:lang="en">Color characteristics are not applicable because the item has no images.</skos:definition>
				<skos:notation xml:lang="en">n</skos:notation>
				<skos:scopeNote xml:lang="en">Used, for example, when the item in hand is sound track film intended to accompany visual images not actually present.</skos:scopeNote>
		</skos:Concept>
<!-- Concept: other  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturecol#z" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturecol#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="3545"/>
				<skos:prefLabel xml:lang="en">other</skos:prefLabel>
				<skos:definition xml:lang="en">Color other than black-and-white, multicolored, hand colored, mixed.</skos:definition>
				<skos:notation xml:lang="en">z</skos:notation>
		</skos:Concept>
<!-- Concept: unknown  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturecol#u" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturecol#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="3544"/>
				<skos:prefLabel xml:lang="en">unknown</skos:prefLabel>
				<skos:definition xml:lang="en">Color characteristics of the motion picture are not known.</skos:definition>
				<skos:notation xml:lang="en">u</skos:notation>
		</skos:Concept>

<!-- Status properties used in this document  -->
    <skos:Concept rdf:about="http://metadataregistry.org/uri/RegStatus/1001">
        <skos:prefLabel xml:lang="en">Published</skos:prefLabel>
    </skos:Concept>

</rdf:RDF>