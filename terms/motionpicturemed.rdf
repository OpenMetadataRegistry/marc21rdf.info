<?xml version="1.0" encoding = "UTF-8"?>
<rdf:RDF
    xmlns="http://www.w3.org/2004/02/skos/core#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:reg="http://metadataregistry.org/uri/schema/registry/">

<!-- Scheme: MARC21-007: Medium for sound of motion pictures -->
    <skos:ConceptScheme rdf:about="http://marc21rdf.info/terms/motionpicturemed#">
        <dc:title>MARC21-007: Medium for sound of motion pictures</dc:title>
    </skos:ConceptScheme>

<!-- Concept: magnetic audio tape in cartridge  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturemed#c" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturemed#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="4000"/>
				<skos:prefLabel xml:lang="en">magnetic audio tape in cartridge</skos:prefLabel>
				<skos:definition xml:lang="en">Accompanying sound is carried on a magnetic audio tape cartridge.</skos:definition>
				<skos:notation xml:lang="en">c</skos:notation>
		</skos:Concept>
<!-- Concept: magnetic audio tape in cassette  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturemed#f" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturemed#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="4003"/>
				<skos:prefLabel xml:lang="en">magnetic audio tape in cassette</skos:prefLabel>
				<skos:definition xml:lang="en">Sound to accompany an item is carried on a cassette of magnetic audio tape.</skos:definition>
				<skos:notation xml:lang="en">f</skos:notation>
		</skos:Concept>
<!-- Concept: magnetic audio tape on reel  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturemed#e" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturemed#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="4002"/>
				<skos:prefLabel xml:lang="en">magnetic audio tape on reel</skos:prefLabel>
				<skos:definition xml:lang="en">Sound to accompany an item is carried on a reel of magnetic audio tape.</skos:definition>
				<skos:notation xml:lang="en">e</skos:notation>
		</skos:Concept>
<!-- Concept: magnetic sound track on motion picture film  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturemed#b" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturemed#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="3999"/>
				<skos:prefLabel xml:lang="en">magnetic sound track on motion picture film</skos:prefLabel>
				<skos:definition xml:lang="en">Sound to accompany a motion picture is carried on a separate magnetic track that is part of the film.</skos:definition>
				<skos:notation xml:lang="en">b</skos:notation>
				<skos:scopeNote xml:lang="en">70 mm. motion picture films often include magnetic sound tracks.</skos:scopeNote>
		</skos:Concept>
<!-- Concept: no sound (silent)  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturemed#%23" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturemed#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="3997"/>
				<skos:prefLabel xml:lang="en">no sound (silent)</skos:prefLabel>
				<skos:definition xml:lang="en">Sound is not present (i.e., the item is intended to be silent).</skos:definition>
				<skos:notation xml:lang="en">#</skos:notation>
		</skos:Concept>
<!-- Concept: optical and magnetic sound track on motion picture film  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturemed#g" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturemed#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="4004"/>
				<skos:prefLabel xml:lang="en">optical and magnetic sound track on motion picture film</skos:prefLabel>
				<skos:definition xml:lang="en">Sound to accompany a motion picture is carried on both an optical and magnetic track.</skos:definition>
				<skos:notation xml:lang="en">g</skos:notation>
		</skos:Concept>
<!-- Concept: optical sound track on motion picture film  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturemed#a" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturemed#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="3998"/>
				<skos:prefLabel xml:lang="en">optical sound track on motion picture film</skos:prefLabel>
				<skos:definition xml:lang="en">Sound to accompany a motion picture is carried on an optical track that is part of the film.</skos:definition>
				<skos:notation xml:lang="en">a</skos:notation>
				<skos:scopeNote xml:lang="en">Optical sound is the most commonly used process for 16 and 35 mm. film.</skos:scopeNote>
		</skos:Concept>
<!-- Concept: other  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturemed#z" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturemed#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="4008"/>
				<skos:prefLabel xml:lang="en">other</skos:prefLabel>
				<skos:definition xml:lang="en">Medium for sound other than no sound (silent), optical sound track on motion picture film, magnetic sound track on motion picture film, magnetic audio tape in cartridge, sound disc, magnetic audio tape on reel, magnetic audio tape in cassette, optical and magnetic sound track on motion picture film, videotape, videodisc.</skos:definition>
				<skos:notation xml:lang="en">z</skos:notation>
		</skos:Concept>
<!-- Concept: sound disc  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturemed#d" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturemed#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="4001"/>
				<skos:prefLabel xml:lang="en">sound disc</skos:prefLabel>
				<skos:definition xml:lang="en">Sound to accompany an item is carried on a sound disc.</skos:definition>
				<skos:notation xml:lang="en">d</skos:notation>
				<skos:scopeNote xml:lang="en">Sound discs include 7, 10, and 12 in. vinyl phonograph records and 4 3/4 in. compact discs.</skos:scopeNote>
		</skos:Concept>
<!-- Concept: unknown  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturemed#u" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturemed#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="4007"/>
				<skos:prefLabel xml:lang="en">unknown</skos:prefLabel>
				<skos:definition xml:lang="en">Medium for sound is not known.</skos:definition>
				<skos:notation xml:lang="en">u</skos:notation>
		</skos:Concept>
<!-- Concept: videodisc  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturemed#i" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturemed#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="4006"/>
				<skos:prefLabel xml:lang="en">videodisc</skos:prefLabel>
				<skos:definition xml:lang="en">Sound to accompany an item is included as part of a videodisc.</skos:definition>
				<skos:notation xml:lang="en">i</skos:notation>
				<skos:scopeNote xml:lang="en">Videodiscs are not ordinarily used to record only sound. A distinction must be made between videodiscs (e.g., 12 in. laser-scan videodiscs that include video information) and compact discs that are used to record sound only (e.g., 4 3/4 in. audio compact discs). The technology to physically record video or audio information on digital disc systems is the same.</skos:scopeNote>
		</skos:Concept>
<!-- Concept: videotape  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturemed#h" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturemed#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="4005"/>
				<skos:prefLabel xml:lang="en">videotape</skos:prefLabel>
				<skos:definition xml:lang="en">Sound to accompany an item is included as part of a videotape.</skos:definition>
				<skos:notation xml:lang="en">h</skos:notation>
				<skos:scopeNote xml:lang="en">Videotape is not ordinarily used to record only sound.</skos:scopeNote>
		</skos:Concept>

