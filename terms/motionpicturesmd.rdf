<?xml version="1.0" encoding = "UTF-8"?>
<rdf:RDF
    xmlns="http://www.w3.org/2004/02/skos/core#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:reg="http://metadataregistry.org/uri/schema/registry/">

<!-- Scheme: MARC21-007: Specific material designation of motion picture -->
    <skos:ConceptScheme rdf:about="http://marc21rdf.info/terms/motionpicturesmd#">
        <dc:title>MARC21-007: Specific material designation of motion picture</dc:title>
    </skos:ConceptScheme>

<!-- Concept: film cartridge  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturesmd#c" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturesmd#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="4449"/>
				<skos:prefLabel xml:lang="en">film cartridge</skos:prefLabel>
				<skos:definition xml:lang="en">Encased in a cartridge that has the ends joined together to form a loop that provides playback without rewinding.</skos:definition>
				<skos:notation xml:lang="en">c</skos:notation>
		</skos:Concept>
<!-- Concept: film cassette  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturesmd#f" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturesmd#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="4450"/>
				<skos:prefLabel xml:lang="en">film cassette</skos:prefLabel>
				<skos:definition xml:lang="en">Encased in a cassette that winds and rewinds reel-to-reel.</skos:definition>
				<skos:notation xml:lang="en">f</skos:notation>
		</skos:Concept>
<!-- Concept: film reel  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturesmd#r" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturesmd#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="4452"/>
				<skos:prefLabel xml:lang="en">film reel</skos:prefLabel>
				<skos:definition xml:lang="en">Open reel of motion picture film designed for use with a projector having its own take-up reel.</skos:definition>
				<skos:notation xml:lang="en">r</skos:notation>
				<skos:scopeNote xml:lang="en">Includes sound track film intended to accompany visual images actually not present.</skos:scopeNote>
		</skos:Concept>
<!-- Concept: film roll  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturesmd#o" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturesmd#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="4451"/>
				<skos:prefLabel xml:lang="en">film roll</skos:prefLabel>
				<skos:definition xml:lang="en">A wound length of film not in a reel, cartridge, etc.</skos:definition>
				<skos:notation xml:lang="en">o</skos:notation>
		</skos:Concept>
<!-- Concept: other  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturesmd#z" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturesmd#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="4454"/>
				<skos:prefLabel xml:lang="en">other</skos:prefLabel>
				<skos:definition xml:lang="en">Specific material designation other than film cartridge, film cassette, film roll, film reel.</skos:definition>
				<skos:notation xml:lang="en">z</skos:notation>
		</skos:Concept>
<!-- Concept: unspecified  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/motionpicturesmd#u" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/motionpicturesmd#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="4453"/>
				<skos:prefLabel xml:lang="en">unspecified</skos:prefLabel>
				<skos:definition xml:lang="en">Special material designation for the motion picture is not specified.</skos:definition>
				<skos:notation xml:lang="en">u</skos:notation>
		</skos:Concept>

<!-- Status properties used in this document  -->
    <skos:Concept rdf:about="http://metadataregistry.org/uri/RegStatus/1001">
        <skos:prefLabel xml:lang="en">Published</skos:prefLabel>
    </skos:Concept>

</rdf:RDF>