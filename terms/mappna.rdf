				<skos:notation xml:lang="en">n</skos:notation>
		</skos:Concept>
<!-- Concept: positive  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/mappna#a" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/mappna#"/>
