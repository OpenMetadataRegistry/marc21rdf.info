<!-- Concept: mixed polarity  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/mappna#m" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/mappna#"/>
				<reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
				<reg:identifier rdf:resource="4175"/>
				<skos:prefLabel xml:lang="en">mixed polarity</skos:prefLabel>
				<skos:definition xml:lang="en">Mixture of positive and negative images.</skos:definition>
				<skos:notation xml:lang="en">m</skos:notation>
		</skos:Concept>
<!-- Concept: negative  -->
		<skos:Concept rdf:about="http://marc21rdf.info/terms/mappna#b" xml:lang="en">
				<skos:inScheme rdf:resource="http://marc21rdf.info/terms/mappna#"/>
